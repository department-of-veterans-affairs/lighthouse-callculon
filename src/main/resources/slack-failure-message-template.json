{
  "channel" : "${channel}",
  "blocks" : [
    {
      "type" : "section",
      "text" : {
        "type" : "mrkdwn",
        "text" : ":x: *ERROR*: Call for *${name}* failed with *${statusCode}* response."
      }
    },
    {
      "type" : "section",
      "text" : {
        "type" : "mrkdwn",
        "text" : "${url}"
      }
    },
    {
      "type" : "context",
      "elements" : [
        {
          "type" : "mrkdwn",
          "text" : "${note}"
        }
      ]
    },
    {
      "type" : "context",
      "elements" : [
        {
          "type" : "mrkdwn",
          "text" : "*${product} ${version}* is scheduled as *${cron}* and was deployed by *${deploymentId}*"
        }
      ]
    }
  ]
